<!-- SCENARIO FOR TESTING THE CONNECTION OF GREEN SOURCE TO SERVER-->
<ScenarioEventsArgs>
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>1</occurrenceTime>
		<name>Client1</name>
		<jobId>1</jobId>
		<start>3</start>
		<end>8</end>
		<deadline>15</deadline>
		<power>10</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB IS ACCEPTED BY SERVER AND WIND FARM 1 (IT'S CHEAPER) -->
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>2</occurrenceTime>
		<name>Client2</name>
		<jobId>2</jobId>
		<start>3</start>
		<end>8</end>
		<deadline>8</deadline>
		<power>10</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB IS REJECTED -->
	<event type="POWER_SHORTAGE_EVENT">
		<occurrenceTime>6</occurrenceTime>
		<agentName>WindFarm1</agentName>
		<newMaximumCapacity>0</newMaximumCapacity>
		<isFinished>false</isFinished>
		<cause>PHYSICAL_CAUSE</cause>
	</event>
	<event type="POWER_SHORTAGE_EVENT">
		<occurrenceTime>10</occurrenceTime>
		<agentName>WindFarm1</agentName>
		<newMaximumCapacity>300</newMaximumCapacity>
		<isFinished>true</isFinished>
		<cause>PHYSICAL_CAUSE</cause>
	</event>
	<!-- EXPECTED OUTCOME: WIND FARM 1 WILL HAVE LOWER PROBABILITY OF BEING CHOSEN, WIND FARM 2 WILL BE PICKED -->
	<event type="NEW_CLIENT_EVENT">
		<occurrenceTime>11</occurrenceTime>
		<name>Client3</name>
		<jobId>3</jobId>
		<start>11</start>
		<end>16</end>
		<deadline>150</deadline>
		<power>10</power>
	</event>
	<!-- EXPECTED OUTCOME: JOB IS ACCEPTED BY SERVER AND WIND FARM 2-->
</ScenarioEventsArgs>
